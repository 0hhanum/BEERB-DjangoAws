container_commands:
  01_migrate:
    command: "django-admin migrate"
    leader_only: true
  02_compilemessages:
    command: "django-admin compilemessages"
  03_createsu:
    command: "django-admin createsu"
    leader_only: true
  04_collectstatic:
    command: "django-admin collectstatic --noinput"
  05_seed_users:
    command: "django-admin seed-users --number 50"
  06_seed_amenities:
    command: "django-admin seed-amenities"
  07_seed_facilities:
    command: "django-admin seed-facilities"    
  
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
  