container_commands:
  01_migrate:
    command: "django-admin migrate"
    leader_only: true
  02_compilemessages:
    command: "django-admin compilemessages"
  03_createsu:
    command: "django-admin createsu"
    leader_only: true
  04_collectstatic:
    command: "django-admin collectstatic --noinput"
  041_seed_rooms:
    command: "django-admin seed_rooms --number 100"
  05_seed_reviews:
    command: "django-admin seed_reviews --number 500"
  06_seed_reservations:
    command: "django-admin seed_reservations --total 220"
  
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
  